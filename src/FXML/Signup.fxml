      <Label layoutX="159.0" layoutY="74.0" text="Enter Name" />
      <Button layoutX="510.0" layoutY="26.0" mnemonicParsing="false" onAction="#logInUIonClick" prefHeight="25.0" prefWidth="67.0" text="Login" />
   </children>
